@article{winter_2017,
	abstract = {Rocky reef coastlines typically feature highly variable and often abrupt cross-shore and alongshore changes in bathymetry. The effects of this irregular rocky bathymetry on the dynamics of infragravity waves are largely unknown. Most models of infragravity wave dynamics have been developed and validated on smooth alongshore-uniform bathymetries, which may break down over these highly variable bathymetries. A 2 week field experiment was conducted on a rocky reef-fringed beach to investigate how the variable bathymetry affects the spatial and temporal variability of infragravity waves. The height of short (sea-swell) waves decreased over the shallow reef due to breaking, whereas the height of infragravity waves increased toward the shoreline. Both during a storm event (Hm0 5 2.3 m) and under moderate wave conditions (Hm0 5 1.0--1.8 m), the infragravity waves formed a persistent cross-shore standing wave pattern along the entire shoreline, despite the irregular bathymetry. In addition, the alongshore components of infragravity waves refracted by the presence of the nearshore reef were observed to propagate in opposite directions up and down the coast resulting in a local alongshore standing wave pattern. Thus, the presence of highly variable nearshore bathymetry, which commonly occurs along rocky reef coastlines, may produce both cross-shore and alongshore standing wave patterns.},
	author = {Winter, G. and Lowe, R. J. and Symonds, G. and Hansen, J. E. and van Dongeren, A. R.},
	doi = {10.1002/2016JC012242},
	file = {Winter et al. - 2017 - Standing infragravity waves over an alongshore irr.pdf:/Users/fengyanshi/Zotero/storage/AZU5ES3P/Winter et al. - 2017 - Standing infragravity waves over an alongshore irr.pdf:application/pdf},
	issn = {21699275},
	journal = {Journal of Geophysical Research: Oceans},
	language = {en},
	month = jun,
	number = {6},
	pages = {4868--4885},
	shorttitle = {Standing infragravity waves over an alongshore irregular rocky bathymetry},
	title = {Standing infragravity waves over an alongshore irregular rocky bathymetry: {INFRAGRAVITY} {WAVES} {OVER} {ROCKY} {BATHYMETRY}},
	url = {http://doi.wiley.com/10.1002/2016JC012242},
	urldate = {2021-11-23},
	volume = {122},
	year = {2017},
