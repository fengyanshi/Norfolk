	journal = {Journal of Physical Oceanography},
	language = {en},
	month = feb,
	number = {2},
	pages = {231--239},
	title = {Gravity and {Shear} {Wave} {Contributions} to {Nearshore} {Infragravity} {Motions}},
	url = {http://journals.ametsoc.org/doi/10.1175/1520-0485(1999)029<0231:GASWCT>2.0.CO;2},
